@using RockyProject.Utility

@if(TempData[WC.Success] != null){
    <script src="~/lib.jquery/dist/jquery.min.js"></script>
    <script src="https://cdnjs.cloudfare.com/ajax/libs/toastr.js/latest.js/js/toastr.min.js"></script>
    <script type="text/javascript">
           toastr.success('@TempData[WC.Success]');    
    </script>
}

@if (TempData[WC.Error] != null)
{
    <script src="~/lib.jquery/dist/jquery.min.js"></script>
    <script src="https://cdnjs.cloudfare.com/ajax/libs/toastr.js/latest.js/js/toastr.min.js"></script>
    <script type="text/javascript">
        toastr.success('@TempData[WC.Error]');
    </script>
}
